#include <iostream>
#include <string.h>
using namespace std;
int main()
{
const int len=80;
char ch[len];
int i;
int numberofWords=0;
int n=0;
cin.get(ch,len);
int length=strlen(ch);
for (i=0;i<length;i++)
{
if(ch[i+1]==' ')
{
if(ch[i]== 'a')
{
n++;
if(n>=2)
{
numberofWords++;
}
}
}
}
cout<<numberofWords<<endl;
return 0;
}
